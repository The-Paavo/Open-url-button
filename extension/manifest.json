{
    "manifest_version": 3,
    "default_locale": "en",
    "name": "__MSG_extensionName__",
    "description": "__MSG_extensionDesc__",
    "version": "1.0.0",
    "minimum_chrome_version": "88",
    "icons": {
        "48": "./icon48.png",
        "128": "./icon128.png"
    },
    "action": {
        "default_icon": {
            "48": "./icon48.png",
            "128": "./icon128.png"
        }
    },
    "options_page": "options.html",
    "background": {
        "service_worker": "service_worker.js"
    },
    "permissions": [
        "tabs",
        "storage"
    ]
}
